{
  "name": "bandit-pouch",
  "version": "2.5.12",
  "description": "Common React components and utilities",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "npm --prefix docs/ start",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose",
    "test:coverage": "jest --verbose --coverage",
    "test:snapshots": "npm --prefix docs/ test",
    "test:snapshots:coverage": "npm --prefix docs/ run test:coverage",
    "test:snapshots:update": "npm --prefix docs/ run test:update",
    "coveralls": "coveralls",
    "css": "postcss --e production -o dist/bandit-pouch.css css/bandit-pouch.css",
    "cjs": "webpack --env cjs",
    "esm": "cross-env NODE_ENV=production babel src -d dist/esm --copy-files",
    "build": "npm run lint && npm run css && npm run cjs && npm run esm",
    "release": "np",
    "release:docs": "npm --prefix docs/ run deploy",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run release:docs"
  },
  "author": "Vladimir Spasic",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kayak/bandit-pouch.git"
  },
  "bugs": {
    "url": "https://github.com/kayak/bandit-pouch/issues"
  },
  "homepage": "https://kayak.github.io/bandit-pouch",
  "license": "Apache-2.0",
  "files": [
    "dist/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
